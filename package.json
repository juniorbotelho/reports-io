{
  "name": "reports.io",
  "version": "0.1.0",
  "private": true,
  "main": "server/Http",
  "scripts": {
    "create:model": "yarn typeorm entity:create -n",
    "create:subscriber": "yarn typeorm subscriber:create -n",
    "create:migration": "yarn typeorm migration:generate -- -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show",
    "migration:drop": "yarn typeorm schema:drop",
    "server:dev": "nodemon --watch './**/*.ts' --ignore './src/**/*.{ts,tsx}' --exec 'ts-node --project server/tsconfig.json -r tsconfig-paths/register' server/Http.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "next:build": "next build",
    "next:dev": "next dev",
    "next:start": "next start",
    "typeorm": "TS_NODE_PROJECT=tsconfig.node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js",
    "build": "npm run next:build && npm run server:build",
    "start": "NODE_ENV=production node dist/server/Http.js"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "firebase": "^8.2.3",
    "fs-extra": "^9.1.0",
    "mysql": "^2.14.1",
    "next": "10.0.5",
    "next-images": "^1.6.2",
    "nodemailer": "^6.4.17",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-lottie": "^1.2.3",
    "reflect-metadata": "^0.1.10",
    "styled-components": "^5.1.1",
    "typeorm": "0.2.29",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@k-foss/ts-esnode": "^1.7.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^8.0.29",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^16.9.46",
    "@types/react-lottie": "^1.2.5",
    "@types/react-spinkit": "^3.0.6",
    "@types/styled-components": "^5.1.2",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.0",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.3.3333"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
